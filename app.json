{
  "expo": {
    "owner": "elemane",
    "name": "Block-immo-front",
    "slug": "Block-immo-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.myfirstapp.souhail",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myfirstapp.souhail",
      "config": {
        "googleServicesFile": "./GoogleService-Info.plist"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.myfirstapp.souhail"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.286675722751-vsaoh02v26ck57mqllncfcgmsfkrdcq3"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
